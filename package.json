{
  "name": "gdg-siliguri-website",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "config": "ts-node ./src/assets/scripts/setEnv.ts",
    "start": "npm run config -- --environment=dev && ng serve",
    "build": "npm run config -- --environment=prod && ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "dev:ssr": "npm run config -- --environment=dev && ng run gdg-siliguri-website:serve-ssr",
    "serve:ssr": "npm run config -- --environment=prod && node dist/gdg-siliguri-website/server/main.js",
    "build:ssr": "npm run config -- --environment=prod && ng build && ng run gdg-siliguri-website:server",
    "prerender": "npm run config -- --environment=prod && ng run gdg-siliguri-website:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.9",
    "@angular/common": "^16.2.9",
    "@angular/compiler": "^16.2.9",
    "@angular/core": "^16.2.9",
    "@angular/fire": "^7.6.1",
    "@angular/forms": "^16.2.9",
    "@angular/platform-browser": "^16.2.9",
    "@angular/platform-browser-dynamic": "^16.2.9",
    "@angular/platform-server": "^16.2.9",
    "@angular/router": "^16.2.9",
    "@angular/service-worker": "^16.2.9",
    "@nguniversal/express-engine": "^16.2.0",
    "express": "^4.15.2",
    "flowbite": "^1.8.1",
    "rxjs": "~7.8.0",
    "ssr-window": "^4.0.2",
    "swiper": "^10.3.1",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.6",
    "@angular/cli": "~16.2.6",
    "@angular/compiler-cli": "^16.2.9",
    "@nguniversal/builders": "^16.2.0",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~4.3.0",
    "@types/node": "^16.11.7",
    "autoprefixer": "^10.4.16",
    "dotenv": "^16.3.1",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "postcss": "^8.4.30",
    "source-map-explorer": "^2.5.3",
    "tailwindcss": "^3.3.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "yargs": "^17.7.2"
  }
}
